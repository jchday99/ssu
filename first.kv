    Screen:
        name: 'menu1'
        BoxLayout:
            orientation: 'vertical'
            BoxLayout:
                orientation: 'horizontal'

                Label:
                    id: hard111
                Button:
                    text: 'Burn 700kcal?'

                    on_press:
                        hard111.text = str(float(700))
                Button:
                    text: 'Yes!'
                    on_press: _screen_manager.current = 'hard'

            BoxLayout:
                orientation: 'horizontal'
                Label:
                    id: med111
                Button:
                    text: 'Burn half?'
                    on_press: med111.text = str(float(0.5)*(700+float(_tcalorie.text)-float(_result.text)))
                Button:
                    text: 'Yes!'
                    on_press: _screen_manager.current = 'medium'

            BoxLayout:
                orientation: 'horizontal'
                Label:
                    id: easy111
                Button:
                    text: 'Burn little?'
                    on_press: easy111.text = app.burn_little(_tcalorie.text, _result.text)
                Button:
                    text: 'Yes!'
                    on_press: _screen_manager.current = 'easy'

            BoxLayout:
                orientation: 'horizontal'
                Button:
                    text: 'Back'
                    on_press: _screen_manager.current = 'screen2'


