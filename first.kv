ScreenManager:
    id: _screen_manager
    Screen:
        name: 'screen1'
        GridLayout:
            cols:1
            TextInput:
                id: _weight_input
                hint_text: 'weight'
                input_filter: 'float'
            TextInput:
                id: _height_input
                hint_text: 'height'
                input_filter: 'float'
            TextInput:
                id: _age_input
                hint_text: 'age'
                input_filter: 'int'
            Label:
                id: _result
            Button:
                text: 'MAN BMR'
                on_press: _result.text = app.calculateManBMI(_weight_input.text, _height_input.text, _age_input.text)
            Button:
                text: 'WOMAN BMR'
                on_press: _result.text = app.calculateWomanBMI(_weight_input.text, _height_input.text, _age_input.text)
            Button:
                text: 'NEXT'
                on_press: _screen_manager.current = 'screen2'
    Screen:
        name: 'screen2'
        GridLayout:
            cols:2
            Button:
                text: 'Breakfast'
                on_press: _screen_manager.current = 'deep1'
            Label:
                id:_calorie1

            Button:
                text: 'Lunch'
                on_press: _screen_manager.current = 'deep2'
            Label:
                id:_calorie2
            Button:
                text: 'Dinner'
                on_press: _screen_manager.current = 'deep3'
            Label:
                id:_calorie3
            Button:
                text:'sum calorie'
                on_press: _tcalorie.text = str(float(_calorie1.text)+float(_calorie2.text)+float(_calorie3.text))
            Label:
                id:_tcalorie
            Button:
                text: 'Back'
                on_press: _screen_manager.current = 'screen1'
            Button:
                text: 'Go To Exercise'
                on_press: _screen_manager.current = 'menu1'
    Screen:
        name: 'deep1'
        GridLayout:
            cols:1
            Label:
                text: "Predicted Class Appears Here."
                font_size: 30
                id: label11
            Image:
                id: img11
                source: ""

            FileChooserListView:
                id: filechooser
                on_selection: app.image_select(filechooser.selection)


            Button:
                text: "Classify Image."
                font_size: 30
                on_press: app.classify_image11()
            Button:
                text: "OK"
                font_size: 30
                on_press:
                    _screen_manager.current = 'screen2'

    Screen:
        name: 'deep2'
        GridLayout:
            cols:1
            Label:
                text: "Predicted Class Appears Here."
